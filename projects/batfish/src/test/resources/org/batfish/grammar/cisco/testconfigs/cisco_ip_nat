!
hostname cisco_ip_nat
!
ip nat pool abc prefix-length 32  
 range 10.1.2.3 10.1.2.4
!
!
ip nat pool example-1-dynamic-nat-pool 192.168.0.128 192.168.0.254 prefix-length 24
!
ip nat pool example-2-dynamic-nat-pool 192.168.0.128 192.168.0.254 netmask 255.255.255.0
!
ip NAT pool dynamic-srcnat 0.0.0.0 0.0.0.0
!
!


! additional rules with trailing optional keywords
ip nat inside source static 99.99.99.99 98.98.98.98 no-alias
ip nat inside source static network 99.99.99.0 98.98.98.0 /24 no-alias
ip nat outside source static 97.97.97.97 96.96.96.96 add-route no-alias
ip nat outside  source static network 97.97.97.0 97.97.97.0 /24 add-route no-alias
ip nat pool out-src-nat-pool 97.97.97.1 97.97.79.254 prefix-length 24 add-route no-alias
