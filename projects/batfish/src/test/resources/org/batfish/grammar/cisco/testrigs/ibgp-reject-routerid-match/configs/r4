! boot system flash:EOS.swi
hostname r4
!
interface Ethernet1
   no switchport
   ip address 10.14.0.4/24
!
interface Ethernet2
   no switchport
   ip address 10.24.0.4/24
!
interface Ethernet3
   no switchport
   ip address 10.34.0.4/24
!
interface Loopback0
   ip address 4.0.0.1/32
!
ip routing
!
router bgp 1
   router-id 4.4.4.4
   neighbor 10.14.0.1 remote-as 1
   neighbor 10.14.0.1 update-source Loopback0
   neighbor 10.14.0.1 route-reflector-client
   neighbor 10.24.0.2 remote-as 1
   neighbor 10.24.0.2 update-source Loopback0
   neighbor 10.24.0.2 route-reflector-client
   neighbor 10.34.0.3 remote-as 1
   neighbor 10.34.0.3 update-source Loopback0
   neighbor 10.34.0.3 route-reflector-client
   network 4.0.0.1/32
!
!
end
