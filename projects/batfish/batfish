#!/bin/bash
BATFISH=${BASH_SOURCE[0]}
BATFISH_PATH=$(dirname $BATFISH)
BATFISH_JAR=$BATFISH_PATH/out/batfish.jar
if batfish_cygwin; then
   BATFISH_JAR="$(cygpath -w $BATFISH_JAR)"
fi
if [ -n "$BATFISH_PLUGIN_DIRS" ]; then
   BATFISH_PLUGIN_DIRS_ARG="-plugindirs ${BATFISH_PLUGIN_DIRS}"
else
   BATFISH_PLUGIN_DIRS_ARG=""
fi
java ${BATFISH_JAVA_OPTS} -DbatfishQuestionPluginDir="${BATFISH_JAVA_QUESTION_PLUGIN_DIR}"  -jar "$BATFISH_JAR" "$BATFISH_PLUGIN_DIRS_ARG" "$@"

