    mtu 1500
  class type network-qos c-foob-ndrop-fcoe
    pause
    no congestion-control random-detect
    mtu 2112
  class type network-qos c-foob-ndrop
    pause
    no congestion-control random-detect
    mtu 2112
policy-map type queueing qqqqq
  class type queuing bleefee
    priority level 1
    bandwidth remaining percent 14
