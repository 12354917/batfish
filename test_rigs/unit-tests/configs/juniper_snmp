#
set system host-name juniper_snmp
set snmp community "dummycommunity" authorization read-only
set snmp community "dummycommunity" clients 1.2.3.0/24
set snmp traceoptions flag pdu
set snmp trap-group trap-destinations categories authentication
set snmp trap-group trap-destinations categories chassis
set snmp trap-group trap-destinations categories link
set snmp trap-group trap-destinations categories routing
set snmp trap-group trap-destinations categories startup
set snmp trap-group trap-destinations categories vrrp-events
set snmp trap-group trap-destinations targets 1.2.3.4
set snmp trap-group trap-destinations targets 2.3.4.5
set snmp trap-options source-address 4.5.6.7
set snmp trap-options source-address 8.9.0.1
set snmp trap-options source-address 5.3.7.4
set snmp trap-options source-address 8.5.3.3
#

